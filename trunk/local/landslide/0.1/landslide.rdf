<rdf:Description rdf:about="http://purl.org/ifgi/landslide#landslide">
	<rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
	<rdfs:label>http://purl.org/ifgi/landslide#landslide</rdfs:label>
	<rdfs:subClassOf rdf:resource="http://purl.org/ifgi/geoevents#ground_movement"/>
	<dc:identifier rdf:datatype="http://www.w3.org/2001/XMLSchema#string">landslide@en</dc:identifier>
	<dc:identifier rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Hangrutsch@de</dc:identifier>
	<dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">a slide of a large mass of dirt and rock down a mountain or cliff</dc:description>
	<dc:source rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://wordnetweb.princeton.edu/</dc:source>
</rdf:Description>

<rdf:Description rdf:about="http://purl.org/ifgi/landslide#deep_landslide">
