	rdfs:subClassOf ge:geographical_event ;
	dc:identifier "landslide@en"^^xsd:string , "Hangrutsch@de"^^xsd:string ;
	dc:description "a slide of a large mass of dirt and rock down a mountain or cliff"^^xsd:string ;
