<rdf:Description rdf:about="http://purl.org/ifgi/rivermodel#Line">
	<rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
	<rdfs:label>http://purl.org/ifgi/rivermodel#Line</rdfs:label>
	<rdfs:subClassOf rdf:resource="http://swing.brgm.fr/repository/ontologies/GML212/current#LineString"/>
	<dc:identifier rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Linie@de</dc:identifier>
	<dc:identifier rdf:datatype="http://www.w3.org/2001/XMLSchema#string">line@en</dc:identifier>
	<dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A line can be described as an ideal zero-width, infinitely long, perfectly straight curve (the term curve in mathematics includes - straight curves) containing an infinite number of points.</dc:description>
	<dc:source rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://en.wikipedia.org/wiki/Line_(mathematics)</dc:source>
	<dc:subject rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Geometry</dc:subject>
	<rdfs:comments rdf:datatype="http://www.w3.org/2001/XMLSchema#string">consists of 2 points as a minimum</rdfs:comments>
</rdf:Description>

