	<dc:identifier rdf:datatype="http://www.w3.org/2001/XMLSchema#string">air@en</dc:identifier>
	<dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A mixture of gases (especially nitrogen and oxygen) in the atmosphere of Earth which is required for breathing and photosynthesis.</dc:description>
	<dc:source rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://en.wikipedia.org/wiki/Air</dc:source>
</rdf:Description>

