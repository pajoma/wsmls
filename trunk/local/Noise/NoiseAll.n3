:TrafficSystem a rdfs:Class ;
	rdfs:subClassOf geo:GeographicSystem ;
	rdfs:label "Vekehrssystem@de"^^xsd:string , "traffic system@en"^^xsd:string ;
	dc:description """Consists of the traffic infrastructure and all means
			of transportation. Includes the activites who cause the traffic, the facilities
			who organize the transport process and the conveyed goods and passengers"""^^xsd:string ;
	dc:source "http://de.wikipedia.org/wiki/Verkehrssystem"^^xsd:string ;
	dc:subject "Systems, Traffic"^^xsd:string ;
	rdfs:comments ""^^xsd:string .

patterns:involvedPhysicalEndurant a rdf:Property ;
	rdfs:domain :TrafficSystem ;
	rdfs:range :Vehicle , :RoadNetwork .

patterns:involvedPerdurant a rdf:Property ;
	rdfs:domain :TrafficSystem ;
	rdfs:range :Pollution , :TrafficFlow , :SoundEmission .

:Infrastructure a rdfs:Class ;
	rdfs:subClassOf dolce:collective ;
	rdfs:label "Infrastruktur@de"^^xsd:string , "infrastructure@en"^^xsd:string ;
	dc:description """refers to the features (facilities/services) of a system
			that support a society"""^^xsd:string ;
	dc:source "http://en.wikipedia.org/wiki/Infrastructure"^^xsd:string , "http://wordnet.princeton.edu/perl/webwn?s=infrastructure"^^xsd:string , "http://www.dsokolov.com/e107_plugins/content/content.php?content.13"^^xsd:string ;
	dc:subject "Collectives"^^xsd:string ;
	rdfs:comments ""^^xsd:string .

dolce:consists-of a rdf:Property ;
	rdfs:domain :Infrastructure ;
	rdfs:range :RoadNetwork .

:NoiseImissionEnvironment a rdfs:Class ;
	rdfs:subClassOf geo:GeographicalUnit ;
	rdfs:label "Laermeinzugsgebiet@de"^^xsd:string , "noise imission environment@en"^^xsd:string ;
	dc:description "describes the environment of an receiver where the noise arrives"^^xsd:string ;
	dc:source "http://www.muelheim-ruhr.de/cms/technische_grundbegriffe__glossar1.html"^^xsd:string ;
	dc:subject "Environment, Accoustics"^^xsd:string ;
	rdfs:comments ""^^xsd:string .

:RoadNetwork a rdfs:Class ;
	rdfs:subClassOf dolce:physical-endurant ;
	rdfs:label "Steckennetz@de"^^xsd:string , "road network@en"^^xsd:string ;
	dc:description """The overall framework for transportation mobility as it
			specifically relates to roads and their hierarchy of principal arterial, minor arterial,
			collector streets and local streets. The road network provides access and allows for the
			movement of people, goods and services through an area. It also includes opportunities
			for alternative modes of transportation such as bicycle lanes and pedestrian pathways."""^^xsd:string ;
	dc:source "www.fortmillsc.org/client_resources/compplan/key_terminology.doc"^^xsd:string ;
	dc:subject "Infrastructure, Roads"^^xsd:string ;
	rdfs:comments ""^^xsd:string .

:Junction a rdfs:Class ;
	rdfs:subClassOf dolce:physical-endurant ;
	rdfs:label "Knotenpunkt@de"^^xsd:string , "junction@en"^^xsd:string ;
	dc:description "the place where two or more things come together"^^xsd:string ;
	dc:source """http://wordnet.princeton.edu/perl/webwn?s=junction&sub=Search+
			WordNet&o2=&o0=1&o7=&o5=&o1=1&o6=&o4=&o3=&h=11000000000000000000000000"""^^xsd:string ;
	dc:subject "Infrastructure, Roads"^^xsd:string ;
	rdfs:comments ""^^xsd:string .

:Road a rdfs:Class ;
	rdfs:subClassOf dolce:physical-endurant ;
	rdfs:label "Strecke@de"^^xsd:string , "road@en"^^xsd:string ;
	dc:description "an open way (generally public) for travel or transportation"^^xsd:string ;
	dc:source """http://wordnet.princeton.edu/perl/webwn?o2=&o0=1&o7=&o5=&o1=1&o6
			=&o4=&o3=&s=road&i=1&h=1000000#c"""^^xsd:string ;
	dc:subject "Infrastructure, Roads"^^xsd:string ;
	rdfs:comments ""^^xsd:string .

dolce:part-of a rdf:Property ;
	rdfs:domain :Road ;
	rdfs:range :RoadNetwork .

:Crossing a rdfs:Class ;
	rdfs:subClassOf :Junction ;
	rdfs:label "Kreuzung von Linien@de"^^xsd:string , "crossing@en"^^xsd:string ;
	dc:description """Crossing is a point where two lines (paths or arcs etc.)
			intersect"""^^xsd:string ;
	dc:source """http://wordnet.princeton.edu/perl/webwn?o2=&o0=1&o7=&o5=&o1=
			1&o6=&o4=&o3=&s=crossing"""^^xsd:string ;
	dc:subject "Infrastructure, Roads"^^xsd:string ;
	rdfs:comments ""^^xsd:string .

dolce:part-of a rdf:Property ;
	rdfs:domain :Crossing ;
	rdfs:range :RoadNetwork .

:intersects a rdf:Property ;
	rdfs:label "schneidet@de"^^xsd:string , "intersects@en"^^xsd:string ;
	dc:description ""^^xsd:string ;
	dc:source ""^^xsd:string ;
	rdfs:comments ""^^xsd:string ;
	rdfs:domain :Crossing ;
	rdfs:range :Road .

:Intersection a rdfs:Class ;
	rdfs:subClassOf :Crossing ;
	rdfs:label "Kreuzung@de"^^xsd:string , "intersection@en"^^xsd:string ;
	dc:description "Intersection is a junction where one road/street crosses another"^^xsd:string ;
	dc:source ""^^xsd:string ;
	dc:subject "Infrastructure, Roads"^^xsd:string ;
	rdfs:comments ""^^xsd:string .

:Interchange a rdfs:Class ;
	rdfs:subClassOf :Crossing ;
	rdfs:label "Autobahnkreuz@de"^^xsd:string , "interchange@en"^^xsd:string ;
	dc:description """Interchange is a junction of highways on different levels
			that permits traffic to move from one to another without crossing traffic streams"""^^xsd:string ;
	dc:source """http://wordnet.princeton.edu/perl/webwn?o2=&o0=1&o7=&o5=&o1=1&o
			6=&o4=&o3=&s=interchange"""^^xsd:string ;
	dc:subject "Infrastructure, Roads"^^xsd:string ;
	rdfs:comments ""^^xsd:string .

:Roundabout a rdfs:Class ;
	rdfs:subClassOf :Crossing ;
	rdfs:label "Kreisverkehr@de"^^xsd:string , "roundabout@en"^^xsd:string ;
	dc:description """Roundabout is a road junction at which traffic streams
			circularly around a central island"""^^xsd:string ;
	dc:source """http://wordnet.princeton.edu/perl/webwn?o2=&o0=1&o7=&o5=&o1=
			1&o6=&o4=&o3=&s=roundabout"""^^xsd:string ;
	dc:subject "Infrastructure, Roads"^^xsd:string ;
	rdfs:comments ""^^xsd:string .

:Street a rdfs:Class ;
	rdfs:subClassOf dolce:physical-endurant ;
	rdfs:label "Strasse@de"^^xsd:string , "street@en"^^xsd:string ;
	dc:description "a street is a public thoroughfare in the built environment including sidewalks"^^xsd:string ;
	dc:source "http://en.wikipedia.org/wiki/Street"^^xsd:string , "http://wordnet.princeton.edu/perl/webwn?s=street"^^xsd:string ;
	dc:subject "Infrastructure, Roads"^^xsd:string ;
	rdfs:comments ""^^xsd:string .

dolce:part-of a rdf:Property ;
	rdfs:domain :Street ;
	rdfs:range :Road .

geo:sourroundedBy a rdf:Property ;
	rdfs:domain :Street ;
	rdfs:range geo:Environment .

:StreetSection a rdfs:Class ;
	rdfs:subClassOf dolce:endurant ;
	rdfs:label "Strassensegment@de"^^xsd:string , "street segment@en"^^xsd:string ;
	dc:description "a defined part of a street by yourself or a defined part by a department"^^xsd:string ;
	dc:source ""^^xsd:string ;
	dc:subject "Infrastructure, Roads"^^xsd:string ;
	rdfs:comments ""^^xsd:string .

dolce:part-of a rdf:Property ;
	rdfs:domain :StreetSection ;
	rdfs:range :Street .

:Vehicle a rdfs:Class ;
	rdfs:subClassOf dolce:physical-endurant ;
	rdfs:label "Fahrzeug@de"^^xsd:string , "vehicle@en"^^xsd:string ;
	dc:description "a mobile conveyance for carrying or transporting goods or persons"^^xsd:string ;
	dc:source "http://de.wikipedia.org/wiki/Fahrzeug"^^xsd:string , "http://wordnet.princeton.edu/perl/webwn?s=vehicle"^^xsd:string ;
	dc:subject "Transportation"^^xsd:string ;
	rdfs:comments ""^^xsd:string .

dolce:participates-in a rdf:Property ;
	rdfs:domain :Vehicle ;
	rdfs:range :TrafficFlow , :SoundEmission .

:Vehicles a rdfs:Class ;
	rdfs:subClassOf dolce:set ;
	rdfs:label "Fahrzeuge@de"^^xsd:string , "vehicles@en"^^xsd:string ;
	dc:description "a mobile conveyance for carrying or transporting goods or persons"^^xsd:string ;
	dc:source "http://de.wikipedia.org/wiki/Fahrzeug"^^xsd:string , "http://wordnet.princeton.edu/perl/webwn?s=vehicle"^^xsd:string ;
	dc:subject "Transportation"^^xsd:string ;
	rdfs:comments ""^^xsd:string .

dolce:consists-of a rdf:Property ;
	rdfs:domain :Vehicles ;
	rdfs:range :Vehicle .

dolce:has-quality a rdf:Property ;
	rdfs:domain :Vehicles ;
	rdfs:range :TrafficDensity .

:Car a rdfs:Class ;
	rdfs:subClassOf :Vehicle ;
	rdfs:label "Personenkraftwagen@de"^^xsd:string , "car@en"^^xsd:string ;
	dc:description "a car is a wheeled motor vehicle for transportating passengers"^^xsd:string ;
	dc:source "http://en.wikipedia.org/wiki/Car"^^xsd:string ;
	dc:subject "Transportation, Roads"^^xsd:string ;
	rdfs:comments ""^^xsd:string .

:Tire a rdfs:Class ;
	rdfs:subClassOf dolce:physical-endurant ;
	rdfs:label "Reifen@de"^^xsd:string , "tire@en"^^xsd:string ;
	dc:description """are ring-shaped parts, either pneumatic or solid
			(including rubber, metals and plastic composites),that fit around wheels
			to protect them and enhance their function"""^^xsd:string ;
	dc:source "http://en.wikipedia.org/wiki/Tire"^^xsd:string ;
	dc:subject "Transportation, Roads"^^xsd:string ;
	rdfs:comments ""^^xsd:string .

dolce:part-of a rdf:Property ;
	rdfs:domain :Tire ;
	rdfs:range :Vehicle .

dolce:has-quality a rdf:Property ;
	rdfs:domain :Tire ;
	rdfs:range :TireType .

:TireType a rdfs:Class ;
	rdfs:subClassOf dolce:quality ;
	rdfs:label "Reifenart@de"^^xsd:string , "tire type@en"^^xsd:string ;
	dc:description """Classification of tires into several standard types,
			based on the type of the vehicle they serve"""^^xsd:string ;
	dc:source "http://en.wikipedia.org/wiki/Tire"^^xsd:string ;
	dc:subject "Transportation, Roads, Quality"^^xsd:string ;
	rdfs:comments ""^^xsd:string .

:Building a rdfs:Class ;
	rdfs:subClassOf geo:GeographicalUnit ;
	rdfs:label "Gebaeude@de"^^xsd:string , "building@en"^^xsd:string ;
	dc:description """a fixed construction with walls, foundation, and roof, such as a house,
			factory, or garage"""^^xsd:string ;
	dc:source "http://www.cdc.gov/nceh/publications/books/housing/definitions.htm"^^xsd:string ;
	dc:subject "Construction"^^xsd:string ;
	rdfs:comments ""^^xsd:string .

geo:situatedIn a rdf:Property ;
	rdfs:domain :Building ;
	rdfs:range :UrbanizedArea .

:Tree a rdfs:Class ;
	rdfs:subClassOf geo:GeographicalUnit ;
	rdfs:label "Gebaeude@de"^^xsd:string , "building@en"^^xsd:string ;
	dc:description "a woody plant with one to a few stems and many branches"^^xsd:string ;
	dc:source "http://extension.usu.edu/forestry/UtahForests/Trees_BotanicalGlossary.htm#t"^^xsd:string ;
	dc:subject "Plants"^^xsd:string ;
	rdfs:comments ""^^xsd:string .

geo:situatedIn a rdf:Property ;
	rdfs:domain :Tree ;
	rdfs:range :UrbanizedArea .

:UrbanizedArea a rdfs:Class ;
	rdfs:subClassOf geo:GeographicalUnit ;
	rdfs:label "st�dtisches Gebiet@de"^^xsd:string , "UrbanizedArea@en"^^xsd:string ;
	dc:description "a geographical area constituting a city or town"^^xsd:string ;
	dc:source "http://wordnetweb.princeton.edu/perl/webwn?s=urban%20area"^^xsd:string ;
	dc:subject "Environment"^^xsd:string ;
	rdfs:comments ""^^xsd:string .

:Pollution a rdfs:Class ;
	rdfs:subClassOf dolce:perdurant ;
	rdfs:label "Kontamination@de"^^xsd:string , "pollution@en"^^xsd:string ;
	dc:description """it is the introduction of contaminants into an environment that causes instability,
			disorder, harm or discomfort to the physical systems or living organisms they are in"""^^xsd:string ;
	dc:source "http://en.wikipedia.org/wiki/Pollution"^^xsd:string ;
	dc:subject "Pollution"^^xsd:string ;
	rdfs:comments ""^^xsd:string .

:SoundEmission a rdfs:Class ;
	rdfs:subClassOf patterns:Event ;
	rdfs:label "Ger�uschemission@de"^^xsd:string , "sound emission@en"^^xsd:string ;
	dc:description """the source which sends out sound waves who take injurious
			to health or disturbing effects for human beings and the environment caused by their
			loudness and structure"""^^xsd:string ;
	dc:source "http://de.wikipedia.org/wiki/L%C3%A4rmemission"^^xsd:string ;
	dc:subject "Accoustics"^^xsd:string ;
	rdfs:comments ""^^xsd:string .

patterns:initiates a rdf:Property ;
	rdfs:domain :SoundEmission ;
	rdfs:range :Noise .

patterns:changes a rdf:Property ;
	rdfs:domain :SoundEmission ;
	rdfs:range :Intensity .

:Noise a rdfs:Class ;
	rdfs:subClassOf :Pollution ;
	rdfs:label "L�rm@de"^^xsd:string , "noise@en"^^xsd:string ;
	dc:description ""^^xsd:string ;
	dc:source ""^^xsd:string ;
	dc:subject "Noise"^^xsd:string ;
	rdfs:comments ""^^xsd:string .

dolce:has-quality a rdf:Property ;
	rdfs:domain :Noise ;
	rdfs:range :Intensity .

:TrafficFlow a rdfs:Class ;
	rdfs:subClassOf dolce:perdurant ;
	rdfs:label "Verkehrsfluss@de"^^xsd:string , "traffic flow@en"^^xsd:string ;
	dc:description """measured number of vehicles who cross
			a particular streetline or streetarea per unit of time"""^^xsd:string ;
	dc:source "http://de.wikipedia.org/wiki/Verkehrsfluss"^^xsd:string ;
	dc:subject "Traffic"^^xsd:string ;
	rdfs:comments ""^^xsd:string .

:NoiseBarrier a rdfs:Class ;
	rdfs:subClassOf patterns:Role ;
	rdfs:label "Laermbarriere@de"^^xsd:string , "noise barrier@en"^^xsd:string ;
	dc:description """it is an exterior structure designed to protect/lag sensitive land
			uses from noise pollution which will be emitted from linear and plane sources (e.g. streets)"""^^xsd:string ;
	dc:source "http://en.wikipedia.org/wiki/Noise_barrier"^^xsd:string , "http://de.wikipedia.org/wiki/L%C3%A4rmschutzwand"^^xsd:string ;
	dc:subject "Accoustics"^^xsd:string ;
	rdfs:comments ""^^xsd:string .

patterns:isPlayedBy a rdf:Property ;
	rdfs:domain :NoiseBarrier ;
	rdfs:range :Building , :Tree .

dolce:has-quality a rdf:Property ;
	rdfs:domain :NoiseBarrier ;
	rdfs:range :Refraction , :Reflection , :Diffraction .

:Sound a rdfs:Class ;
	rdfs:subClassOf dolce:endurant ;
	rdfs:label "Schall@de"^^xsd:string , "sound@en"^^xsd:string ;
	dc:description """sound is a wave produced by the vibration of an object
 			transmitted through a solid, liquid, or gas"""^^xsd:string ;
	dc:source "http://activities.macmillanmh.com/science/ca/grade6/glossary/s.html"^^xsd:string , "http://en.wikipedia.org/wiki/Sound_Wave"^^xsd:string ;
	dc:subject "Accoustics"^^xsd:string ;
	rdfs:comments ""^^xsd:string .

dolce:has-quality a rdf:Property ;
	rdfs:domain :Sound ;
	rdfs:range :Intensity .

:Intensity a rdfs:Class ;
	rdfs:subClassOf dolce:quality ;
	rdfs:label "Intensit�t@de"^^xsd:string , "intensity@en"^^xsd:string ;
	dc:description "the amount of energy transmitted"^^xsd:string ;
	dc:source """http://wordnetweb.princeton.edu/perl/webwn?s
			=intensity&sub=Search+WordNet&o2=&o0=1&o7=&o5=&o1=1&o6=&o4=&o3=&h=0"""^^xsd:string ;
	dc:subject "Quality"^^xsd:string ;
	rdfs:comments ""^^xsd:string .

:TrafficDensity a rdfs:Class ;
	rdfs:subClassOf dolce:quality ;
	rdfs:label "Verkehrsdichte@de"^^xsd:string , "traffic density@en"^^xsd:string ;
	dc:description """Traffic density is the number of vehicles of different traffic carrier
			per a specific section of road at a certain point of time"""^^xsd:string ;
	dc:source "http://de.wikipedia.org/wiki/Verkehrsdichte"^^xsd:string ;
	dc:subject "Quality, Traffic"^^xsd:string ;
	rdfs:comments ""^^xsd:string .

:Refraction a rdfs:Class ;
	rdfs:subClassOf dolce:quality ;
	rdfs:label "Brechung@de"^^xsd:string , "refraction@en"^^xsd:string ;
	dc:description """The change in direction of a wave due to change in its speed
			because of a change in the medium it is traveling through"""^^xsd:string ;
	dc:source "http://www.mhschool.com/science/2008/student/na/grade6/glossary/r.html"^^xsd:string , "http://en.wikipedia.org/wiki/Refraction"^^xsd:string ;
	dc:subject "Quality"^^xsd:string ;
	rdfs:comments ""^^xsd:string .

:Reflection a rdfs:Class ;
	rdfs:subClassOf dolce:quality ;
	rdfs:label "Reflektion@de"^^xsd:string , "reflection@en"^^xsd:string ;
	dc:description """the change in direction of a wave at an interface
			between two different media so that the wave will be thrown back from a surface
