<rdf:Description rdf:about="http://swing.uni-muenster.de/core/RiverModel#DistinctivePoint">
	<rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
	<rdfs:subClassOf rdf:resource="http://swing.brgm.fr/repository/ontologies/GML212/current#Point"/>
	<rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">markanter Punkt@de</rdfs:label>
	<rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">distinctive point@en</rdfs:label>
	<dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">a marked point which helps to distinguish other points;&#xD;
			Hypernym for all terrain points</dc:description>
	<dc:source rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://wordnet.princeton.edu/perl/webwn?s=distinctive&amp;sub=Search+WordNet&#xD;
			&amp;o2=&amp;o0=1&amp;o7=&amp;o5=&amp;o1=1&amp;o6=&amp;o4=&amp;o3=&amp;h=00</dc:source>
	<dc:subject rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Geometry</dc:subject>
	<rdfs:comments rdf:datatype="http://www.w3.org/2001/XMLSchema#string"></rdfs:comments>
</rdf:Description>

<rdf:Description rdf:about="http://swing.uni-muenster.de/core/RiverModel#IsometricPoint">
	<rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
	<rdfs:subClassOf rdf:resource="http://swing.brgm.fr/repository/ontologies/GML212/current#Point"/>
	<rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Isopunkt@de</rdfs:label>
	<rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">isometric point@en</rdfs:label>
	<dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">points who are connected with a isometric line</dc:description>
	<dc:source rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://wordnet.princeton.edu/perl/webwn?o2=&#xD;
			&amp;o0=1&amp;o7=&amp;o5=&amp;o1=1&amp;o6=&amp;o4=&amp;o3=&amp;s=isometric+line&amp;i=8&amp;h=111100010#c</dc:source>
	<dc:subject rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Geometry</dc:subject>
	<rdfs:comments rdf:datatype="http://www.w3.org/2001/XMLSchema#string"></rdfs:comments>
</rdf:Description>

<rdf:Description rdf:about="http://swing.uni-muenster.de/core/RiverModel#BreakPoint">
	<rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
	<rdfs:subClassOf rdf:resource="http://swing.uni-muenster.de/core/RiverModel#DistinctivePoint"/>
	<rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Bruchpunkt@de</rdfs:label>
	<rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">break point@en</rdfs:label>
	<dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">points who are connected with a break line</dc:description>
	<dc:source rdf:datatype="http://www.w3.org/2001/XMLSchema#string"></dc:source>
	<dc:subject rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Geometry</dc:subject>
	<rdfs:comments rdf:datatype="http://www.w3.org/2001/XMLSchema#string"></rdfs:comments>
</rdf:Description>

<rdf:Description rdf:about="http://swing.uni-muenster.de/core/RiverModel#Line">
	<rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
	<rdfs:subClassOf rdf:resource="http://swing.brgm.fr/repository/ontologies/GML212/current#LineString"/>
	<rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Linie@de</rdfs:label>
	<rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">line@en</rdfs:label>
	<dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A line can be described as an ideal zero-width,&#xD;
			infinitely long, perfectly straight curve (the term curve in mathematics&#xD;
			includes - straight curves) containing an infinite number of points.</dc:description>
	<dc:source rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://en.wikipedia.org/wiki/Line_(mathematics)</dc:source>
	<dc:subject rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Geometry</dc:subject>
	<rdfs:comments rdf:datatype="http://www.w3.org/2001/XMLSchema#string">consists of 2 points as a minimum</rdfs:comments>
</rdf:Description>

<rdf:Description rdf:about="http://swing.uni-muenster.de/core/RiverModel#DistinctiveLine">
	<rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
	<rdfs:subClassOf rdf:resource="http://swing.brgm.fr/repository/ontologies/GML212/current#LineString"/>
	<rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">markante linie@de</rdfs:label>
	<rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">distinctive line@en</rdfs:label>
	<dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">a marked line which helps to distinguish other lines; &#xD;
			hypernym for all terrain lines e.g. break line</dc:description>
	<dc:source rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://wordnet.princeton.edu/perl/webwn?s=distinctive&#xD;
			&amp;sub=Search+WordNet&amp;o2=&amp;o0=1&amp;o7=&amp;o5=&amp;o1=1&amp;o6=&amp;o4=&amp;o3=&amp;h=00</dc:source>
	<dc:subject rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Geometry</dc:subject>
	<rdfs:comments rdf:datatype="http://www.w3.org/2001/XMLSchema#string"></rdfs:comments>
</rdf:Description>

<rdf:Description rdf:about="http://swing.uni-muenster.de/core/RiverModel#IsometricLine">
	<rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
	<rdfs:subClassOf rdf:resource="http://swing.uni-muenster.de/core/RiverModel#DistinctiveLine"/>
	<rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Isolinie@de</rdfs:label>
	<rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">isometric line@en</rdfs:label>
	<dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">a line connecting isometric points</dc:description>
	<dc:source rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://wordnet.princeton.edu/perl/webwn?s=isometric+line&amp;sub=&#xD;
			Search+WordNet&amp;o2=&amp;o0=1&amp;o7=&amp;o5=&amp;o1=1&amp;o6=&amp;o4=&amp;o3=&amp;h=0</dc:source>
	<dc:subject rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Geometry</dc:subject>
	<rdfs:comments rdf:datatype="http://www.w3.org/2001/XMLSchema#string"></rdfs:comments>
</rdf:Description>

<rdf:Description rdf:about="http://swing.uni-muenster.de/core/RiverModel#SlopeLine">
	<rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
	<rdfs:subClassOf rdf:resource="http://swing.uni-muenster.de/core/RiverModel#DistinctiveLine"/>
	<rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Falllinie@de</rdfs:label>
	<rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">slope line@en</rdfs:label>
	<dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A line on a surface which follows the direction of&#xD;
			the biggest grade; squares contour lines</dc:description>
	<dc:source rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://de.wikipedia.org/wiki/Falllinie</dc:source>
	<dc:subject rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Geometry</dc:subject>
	<rdfs:comments rdf:datatype="http://www.w3.org/2001/XMLSchema#string"></rdfs:comments>
</rdf:Description>

<rdf:Description rdf:about="http://swing.uni-muenster.de/core/RiverModel#StructureLine">
	<rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
	<rdfs:subClassOf rdf:resource="http://swing.uni-muenster.de/core/RiverModel#DistinctiveLine"/>
	<rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Strukturlinie@de</rdfs:label>
	<rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">structure line@en</rdfs:label>
	<dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A line given by the surface of the subaerial&#xD;
			geomorphological unit (break line, crest line)</dc:description>
	<dc:source rdf:datatype="http://www.w3.org/2001/XMLSchema#string"></dc:source>
	<dc:subject rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Geometry</dc:subject>
	<rdfs:comments rdf:datatype="http://www.w3.org/2001/XMLSchema#string"></rdfs:comments>
</rdf:Description>

<rdf:Description rdf:about="http://swing.uni-muenster.de/core/RiverModel#BreakLine">
	<rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
	<rdfs:subClassOf rdf:resource="http://swing.uni-muenster.de/core/RiverModel#StructureLine"/>
	<rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Bruchkante@de</rdfs:label>
	<rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">break line@en</rdfs:label>
	<dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">a line where the grade changes excursivly</dc:description>
	<dc:source rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://de.wikipedia.org/wiki/Gel%C3%A4nde_(Kartografie)</dc:source>
	<dc:subject rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Geometry</dc:subject>
	<rdfs:comments rdf:datatype="http://www.w3.org/2001/XMLSchema#string"></rdfs:comments>
</rdf:Description>

<rdf:Description rdf:about="http://swing.uni-muenster.de/core/RiverModel#derivesFrom">
	<rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
	<rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">stammt ab von@de</rdfs:label>
	<rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">derives from@en</rdfs:label>
	<dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string"></dc:description>
	<rdfs:comments rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A break line consists of break points</rdfs:comments>
	<rdfs:domain rdf:resource="http://swing.uni-muenster.de/core/RiverModel#BreakLine"/>
	<rdfs:range rdf:resource="http://swing.uni-muenster.de/core/RiverModel#BreakPoint"/>
</rdf:Description>

<rdf:Description rdf:about="http://swing.uni-muenster.de/core/RiverModel#ContourLine">
	<rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
	<rdfs:subClassOf rdf:resource="http://swing.uni-muenster.de/core/RiverModel#IsometricLine"/>
	<rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Hoehenlinie@de</rdfs:label>
	<rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">contour line@en</rdfs:label>
	<dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A line used on a topographic map to indicate areas of&#xD;
			equal elevation above sea level</dc:description>
	<dc:source rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://resweb.llu.edu/rford/docs/VGD/GSLVT/gslglossary.html</dc:source>
	<dc:subject rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Geometry</dc:subject>
	<rdfs:comments rdf:datatype="http://www.w3.org/2001/XMLSchema#string"></rdfs:comments>
</rdf:Description>

