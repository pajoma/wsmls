	<rdfs:domain rdf:resource="http://purl.org/ifgi/OilSpill#oil"/>
</rdf:Description>

<rdf:Description rdf:about="http://purl.org/ifgi/OilSpill#crude_oil">
	<rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
	<rdfs:subClassOf rdf:resource="http://purl.org/ifgi/OilSpill#oil"/>
	<rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Erdöl@de</rdfs:label>
	<rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">crude oil@en</rdfs:label>
	<dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Crude oil or petroleum is a naturally occurring, inflammable dark oil consisting mainly of hydrocarbons. It is found in geologic formations beneath the Earth's surface.</dc:description>
	<dc:source rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://wordnetweb.princeton.edu/</dc:source>
</rdf:Description>

<rdf:Description rdf:about="http://purl.org/ifgi/OilSpill#black_oil">
	<rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
