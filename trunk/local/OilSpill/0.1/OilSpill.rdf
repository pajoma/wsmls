<rdf:Description rdf:about="http://purl.org/ifgi/OilSpill#direction">
	<rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
	<rdfs:subClassOf rdf:resource="http://purl.org/net/wsml/dolce#quality"/>
	<rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Richtung@de</rdfs:label>
	<rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">direction@en</rdfs:label>
	<dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Direction is the information contained in the relative position of one object with respect to another object without the distance information. Directions may be either relative to some indicated reference (the violins in a full orchestra are typically seated to the left of the conductor), or absolute according to some previously agreed upon frame of reference (New York City lies due west of Madrid).</dc:description>
	<dc:source rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://en.wikipedia.org/wiki/Direction_(geometry)</dc:source>
</rdf:Description>

<rdf:Description rdf:about="http://purl.org/ifgi/OilSpill#temperature">
	<rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
	<rdfs:subClassOf rdf:resource="http://purl.org/net/wsml/dolce#quality"/>
	<rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Temperatur@de</rdfs:label>
	<rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">temperature@en</rdfs:label>
	<dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Temperature is a physical property of matter (corresponding to its molecular activity) that quantitatively expresses the common notions of hot and cold.</dc:description>
	<dc:source rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://en.wikipedia.org/wiki/Temperature</dc:source>
</rdf:Description>

<rdf:Description rdf:about="http://purl.org/ifgi/OilSpill#velocity">
	<rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
	<rdfs:subClassOf rdf:resource="http://purl.org/net/wsml/dolce#quality"/>
	<rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Geschwindigkeit@de</rdfs:label>
	<rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">velocity@en</rdfs:label>
	<dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Velocity is the distance travelled per unit time of an object.</dc:description>
	<dc:source rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://wordnetweb.princeton.edu/</dc:source>
</rdf:Description>

<rdf:Description rdf:about="http://purl.org/ifgi/OilSpill#mass">
	<rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
	<rdfs:subClassOf rdf:resource="http://purl.org/net/wsml/dolce#quality"/>
	<rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Masse@de</rdfs:label>
	<rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">mass@en</rdfs:label>
	<dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The property of matter that causes it to have weight in a gravitational field. Mass is measured in kilograms (kg).</dc:description>
	<dc:source rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://wordnetweb.princeton.edu/</dc:source>
</rdf:Description>

<rdf:Description rdf:about="http://purl.org/ifgi/OilSpill#viscosity">
	<rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
	<rdfs:subClassOf rdf:resource="http://purl.org/net/wsml/dolce#quality"/>
	<rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Viskosität@de</rdfs:label>
	<rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">viscosity@en</rdfs:label>
	<dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Resistance of a liquid to shear forces (and hence to flow).</dc:description>
	<dc:source rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://wordnetweb.princeton.edu/</dc:source>
</rdf:Description>

<rdf:Description rdf:about="http://purl.org/ifgi/OilSpill#toxicity">
	<rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
	<rdfs:subClassOf rdf:resource="http://purl.org/net/wsml/dolce#quality"/>
	<rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Giftigkeit/Giftgehalt@de</rdfs:label>
	<rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">toxicity@en</rdfs:label>
	<dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The degree to which a substance is poisonous and thus can damage an organism, such as an animal, or plant, as well as a substructure of the organism, such as a cell (cytotoxicity) or an organ.</dc:description>
	<dc:source rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://en.wikipedia.org/wiki/Toxicity</dc:source>
</rdf:Description>

<rdf:Description rdf:about="http://purl.org/ifgi/OilSpill#depth">
	<rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
	<rdfs:subClassOf rdf:resource="http://purl.org/net/wsml/dolce#quality"/>
	<rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Tiefe@de</rdfs:label>
	<rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">depth@en</rdfs:label>
	<dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The vertical distance of an object below the surface of a specified reference point.</dc:description>
	<dc:source rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://en.wiktionary.org/wiki/depth</dc:source>
</rdf:Description>

<rdf:Description rdf:about="http://purl.org/ifgi/OilSpill#concentration">
	<rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
	<rdfs:subClassOf rdf:resource="http://purl.org/net/wsml/dolce#quality"/>
	<rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Konzentration@de</rdfs:label>
	<rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">concentration@en</rdfs:label>
	<dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Concentration is defined as the abundance of a constituent divided by the total volume of a mixture.</dc:description>
	<dc:source rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://en.wikipedia.org/wiki/Concentration</dc:source>
</rdf:Description>

<rdf:Description rdf:about="http://purl.org/ifgi/OilSpill#oil_concentration">
	<rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
