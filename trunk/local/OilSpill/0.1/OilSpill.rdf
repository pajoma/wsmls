	<dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Goods transported by a large vehicle such as ship, aircraft, van, or truck.</dc:description>
	<dc:source rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://wordnetweb.princeton.edu/</dc:source>
</rdf:Description>

<rdf:Description rdf:about="http://purl.org/ifgi/OilSpill#id">
	<rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
	<rdfs:subClassOf rdf:resource="http://purl.org/net/wsml/dolce#quality"/>
	<rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">ID@de</rdfs:label>
