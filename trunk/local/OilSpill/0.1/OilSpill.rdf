<rdf:Description rdf:about="http://purl.org/ifgi/OilSpill#name">
	<rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
	<rdfs:subClassOf rdf:resource="http://purl.org/net/wsml/dolce#quality"/>
	<rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Name@de</rdfs:label>
	<rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">name@en</rdfs:label>
	<dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A word or term used for identification of a person, single thing, or category of things.</dc:description>
	<dc:source rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://en.wikipedia.org/wiki/Name</dc:source>
</rdf:Description>

<rdf:Description rdf:about="http://purl.org/ifgi/OilSpill#cargo">
	<rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
	<rdfs:subClassOf rdf:resource="http://purl.org/net/wsml/dolce#quality"/>
