	<rdfs:domain rdf:resource="http://purl.org/ifgi/OilSpill#water"/>
</rdf:Description>

<rdf:Description rdf:about="http://purl.org/ifgi/OilSpill#oil">
	<rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
	<rdfs:subClassOf rdf:resource="http://purl.org/net/wsml/dolce#amount_of_matter"/>
	<rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Öl@de</rdfs:label>
	<rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">oil@en</rdfs:label>
	<dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A slippery or viscous liquid or liquefiable substance not miscible with water. Oils have a high carbon and hydrogen content and are nonpolar substances.</dc:description>
	<dc:source rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://en.wikipedia.org/wiki/Oil</dc:source>
</rdf:Description>

<rdf:Description rdf:about="http://purl.org/net/wsml/dolce#has_quality">
