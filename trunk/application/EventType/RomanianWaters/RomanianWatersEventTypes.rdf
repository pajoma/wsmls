<rdf:Description rdf:nodeID="node17m1rg2aix1">
	<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Ontology"/>
	<rdfs:label>RomanianWatersEventTypes</rdfs:label>
	<dc:format>http://www.wsmo.org/wsml/wsml-syntax/wsml-flight</dc:format>
	<dc:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Alejandro Llaves</dc:creator>
	<dc:rights rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://creativecommons.org/licenses/by/3.0/</dc:rights>
	<dc:type rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Application Ontology</dc:type>
	<dc:title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Event types for Romanian Waters scenario</dc:title>
	<dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">This ontology contains the event types necessary to annotate the event patterns of the Romanian Waters scenario.</dc:description>
</rdf:Description>

<rdf:Description rdf:about="http://wmls.googlecode.com/svn/trunk/application/EventType/RomanianWaters#AtentionStage">
	<rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
	<rdfs:label>http://wmls.googlecode.com/svn/trunk/application/EventType/RomanianWaters#AtentionStage</rdfs:label>
	<rdfs:subClassOf rdf:resource="http://wsmls.googlecode.com/svn/trunk/local/water/0.6/flood.rdf#WaterLevelChange"/>
	<dc:identifier rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Atention stage@en</dc:identifier>
	<dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The level at which the risk of flooding is possible after a relatively short time frame, requiring increased vigilance when carrying out activities exposed to flooding.</dc:description>
	<dc:source rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.rowater.ro/daprut/Documente%20Repository/Regulament%20%20gestionare%20situatii%20de%20urgenta%20.pdf</dc:source>
</rdf:Description>

<rdf:Description rdf:about="http://wmls.googlecode.com/svn/trunk/application/EventType/RomanianWaters#FloodingStage">
	<rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
	<rdfs:label>http://wmls.googlecode.com/svn/trunk/application/EventType/RomanianWaters#FloodingStage</rdfs:label>
	<rdfs:subClassOf rdf:resource="http://wsmls.googlecode.com/svn/trunk/local/water/0.6/flood.rdf#WaterLevelChange"/>
	<dc:identifier rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Flooding stage@en</dc:identifier>
	<dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The level at which major floods occur can lead to flooding of households and socio-economic goods.</dc:description>
	<dc:source rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.rowater.ro/daprut/Documente%20Repository/Regulament%20%20gestionare%20situatii%20de%20urgenta%20.pdf</dc:source>
</rdf:Description>

<rdf:Description rdf:about="http://wmls.googlecode.com/svn/trunk/application/EventType/RomanianWaters#DangerStage">
	<rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
	<rdfs:label>http://wmls.googlecode.com/svn/trunk/application/EventType/RomanianWaters#DangerStage</rdfs:label>
	<rdfs:subClassOf rdf:resource="http://wsmls.googlecode.com/svn/trunk/local/water/0.6/flood.rdf#WaterLevelChange"/>
	<dc:identifier rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Danger stage@en</dc:identifier>
	<dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The level at which special measures are necessary for the evacuation of people and goods, the restriction on the use of bridges and roads, and the operation of hydraulic structures.</dc:description>
	<dc:source rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.rowater.ro/daprut/Documente%20Repository/Regulament%20%20gestionare%20situatii%20de%20urgenta%20.pdf</dc:source>
</rdf:Description>

<rdf:Description rdf:about="http://wmls.googlecode.com/svn/trunk/application/EventType/RomanianWaters#AttentionThresholdExceeded">
	<rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
	<rdfs:label>http://wmls.googlecode.com/svn/trunk/application/EventType/RomanianWaters#AttentionThresholdExceeded</rdfs:label>
	<rdfs:subClassOf rdf:resource="http://wsmls.googlecode.com/svn/trunk/local/water/0.6/flood.rdf#WaterLevelChange"/>
	<dc:identifier rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Attention threshold exceeded@en</dc:identifier>
	<dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Two observations (a, b) produced by the same sensor and ordered in time where the value of a is below the attention threshold and the value of b is above.</dc:description>
</rdf:Description>

