	<rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
	<rdfs:subClassOf rdf:resource="http://purl.org/net/wsml/dolceDnS#physical_phenomenon"/>
	<rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"></rdfs:label>
	<dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string"></dc:description>
	<dc:source rdf:datatype="http://www.w3.org/2001/XMLSchema#string"></dc:source>
</rdf:Description>

<rdf:Description rdf:about="http://purl.org/net/wsml/geospace#wind">
	<rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
	<rdfs:subClassOf rdf:resource="http://purl.org/net/wsml/geospace#movement"/>
	<rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Wind@de</rdfs:label>
	<rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">wind@en</rdfs:label>
	<dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Moving air caused by differences in pressure. The air moves from an area of high pressure to an area of low pressure.</dc:description>
	<dc:source rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://en.wikipedia.org/wiki/Wind</dc:source>
</rdf:Description>

<rdf:Description rdf:about="http://purl.org/net/wsml/geospace#storm">
	<rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
	<rdfs:subClassOf rdf:resource="http://purl.org/net/wsml/geospace#wind"/>
	<rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Sturm@de</rdfs:label>
	<rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">storm@en</rdfs:label>
	<dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A meteorological definition of a terrestrial storm is a wind measuring 10 or higher on the Beaufort scale. It may be marked by strong wind, hail, thunder and/or lightning (a thunderstorm), heavy precipitation... </dc:description>
	<dc:source rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://en.wikipedia.org/wiki/Storm</dc:source>
</rdf:Description>

<rdf:Description rdf:about="http://purl.org/net/wsml/geospace#flow">
	<rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
	<rdfs:subClassOf rdf:resource="http://purl.org/net/wsml/geospace#movement"/>
	<rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Strömung@de</rdfs:label>
	<rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">flow@en</rdfs:label>
	<dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An uninterrupted motion characteristic of fluids (liquids or gases) such as water</dc:description>
	<dc:source rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://wordnetweb.princeton.edu</dc:source>
</rdf:Description>

<rdf:Description rdf:about="http://purl.org/net/wsml/geospace#river_current">
	<rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
	<rdfs:subClassOf rdf:resource="http://purl.org/net/wsml/geospace#flow"/>
	<rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Flussströmung@de</rdfs:label>
	<rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">river current@en</rdfs:label>
	<dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The steady flow of water in a river influenced by gravity as the water moves downhill to reduce its potential energy.</dc:description>
	<dc:source rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://en.wikipedia.org/wiki/Current_(stream)</dc:source>
</rdf:Description>

<rdf:Description rdf:about="http://purl.org/net/wsml/geospace#sea_current">
	<rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
	<rdfs:subClassOf rdf:resource="http://purl.org/net/wsml/geospace#flow"/>
	<rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Meeresströmung@de</rdfs:label>
	<rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">sea current@en</rdfs:label>
	<dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The steady flow of surface sea water in a prevailing direction.</dc:description>
	<dc:source rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://wordnetweb.princeton.edu/</dc:source>
</rdf:Description>

