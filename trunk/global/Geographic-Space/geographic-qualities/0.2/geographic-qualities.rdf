<rdf:Description rdf:about="http://purl.org/net/wsml/geospace#aspect">
	<rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
	<rdfs:subClassOf rdf:resource="http://purl.org/net/wsml/geospace#geographic_quality"/>
	<rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Aspect@en</rdfs:label>
	<dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">In geography, aspect refers to the horizontal direction to which Slope faces.</dc:description>
</rdf:Description>

<rdf:Description rdf:about="http://purl.org/net/wsml/geospace#altitude">
	<rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
	<rdfs:subClassOf rdf:resource="http://purl.org/net/wsml/geospace#geographic_quality"/>
	<rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Altitude@en</rdfs:label>
	<dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The height or vertical elevation of a point above a reference surface. Altitude measurements are usually based on a given reference datum, such as mean sea level.</dc:description>
	<dc:relation rdf:resource="http://en.wikipedia.org/wiki/Altitude"/>
</rdf:Description>

<rdf:Description rdf:about="http://purl.org/net/wsml/geospace#elevation">
	<rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
	<rdfs:subClassOf rdf:resource="http://purl.org/net/wsml/geospace#geographic_quality"/>
	<rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Hoehe@de</rdfs:label>
	<rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">elevation@en</rdfs:label>
	<dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The elevation of a geographic location is its height above a fixed reference point, often the mean sea level</dc:description>
	<dc:source rdf:datatype="http://www.w3.org/2001/XMLSchema#string">en.wikipedia.org/wiki/Elevation</dc:source>
</rdf:Description>

