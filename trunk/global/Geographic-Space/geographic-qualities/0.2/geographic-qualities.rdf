	<rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Gefaelle@de</rdfs:label>
	<dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The degree to which a surface tends upward or downward.</dc:description>
</rdf:Description>

