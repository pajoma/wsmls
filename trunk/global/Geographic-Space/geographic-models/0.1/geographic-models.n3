	rdfs:domain :PredictedEvent ;
	a rdf:Property ;
	rdfs:range ge:geographical_event .

:PredictedObservation a rdfs:Class ;
	rdfs:label "http://purl.org/ifgi/geomodels#PredictedObservation" ;
