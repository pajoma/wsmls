:SitationDescription a rdfs:Class ;
	rdfs:label "http://purl.org/ifgi/geodesc#SitationDescription" ;
	rdfs:subClassOf dul:Description .

:SitationDescription_prop_describes a rdf:Property ;
	rdfs:label "http://purl.org/ifgi/geodesc#describes" ;
	rdfs:domain :SitationDescription ;
	a rdf:Property ;
	rdfs:range dul:Situation .

:SitationDescription_prop_describedLocation a rdf:Property ;
	rdfs:label "http://purl.org/ifgi/geodesc#describedLocation" ;
	rdfs:domain :SitationDescription ;
	a rdf:Property ;
	rdfs:range :LocationDescription .

:GeographicalDescription a rdfs:Class ;
	rdfs:label "http://purl.org/ifgi/geodesc#GeographicalDescription" ;
	rdfs:subClassOf dul:Description ;
	dc:title "Description of Geographic Unit"^^xsd:string .

:GeographicalDescription_prop_describes a rdf:Property ;
	rdfs:label "http://purl.org/ifgi/geodesc#describes" ;
	rdfs:domain :GeographicalDescription ;
