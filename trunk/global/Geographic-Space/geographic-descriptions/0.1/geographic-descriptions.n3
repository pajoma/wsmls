_:node16odvr70nx1 a owl:Ontology ;
	rdfs:label "geographic_descriptions" ;
	dc:format "http://www.wsmo.org/wsml/wsml-syntax/wsml-flight" ;
	owl:imports <http://purl.org/ifgi/dulSpatial#> , <http://purl.org/ifgi/dul#> , <http://purl.org/ifgi/geospace#> , <http://purl.org/ifgi/time#> ;
