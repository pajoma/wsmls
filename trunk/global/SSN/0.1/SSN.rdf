</rdf:Description>

<rdf:Description rdf:about="http://purl.org/ifgi/ssn#startTime">
	<rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
	<rdfs:label rdf:resource="http://purl.org/ifgi/ssn#startTime"/>
	<rdfs:domain rdf:resource="http://purl.org/ifgi/dul#Entity"/>
	<rdfs:range rdf:resource="http://purl.org/ifgi/dul#Region"/>
