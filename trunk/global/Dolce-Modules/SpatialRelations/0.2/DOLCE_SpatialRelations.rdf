<rdf:Description rdf:about="http://purl.org/net/wsml/dolceSpatial#descriptive_origin">
	<rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
	<rdfs:domain rdf:resource="http://purl.org/net/wsml/dolce#endurant"/>
	<rdfs:range rdf:resource="http://purl.org/net/wsml/dolce#non_physical_endurant"/>
