<rdf:Description rdf:about="http://purl.org/net/wsml/dolceSpatial#place">
	<rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
	<rdfs:domain rdf:resource="http://purl.org/net/wsml/dolce#endurant"/>
	<rdfs:range rdf:resource="http://purl.org/net/wsml/dolce#physical_endurant"/>
	<rdfs:subPropertyOf rdf:resource="http://purl.org/net/wsml/dolceSpatial#approximate_location"/>
	<dc:title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">has place@en</dc:title>
	<dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relation of approximate location holding between any endurant and physical endurants. It assumes a shared physical region. Sharedness can be any mereotopological association, but no analytical definition can be done in OWL due to the lack of role value maps.</dc:description>
	<rdfs:seeAlso rdf:resource="http://www.loa-cnr.it/ontologies/SpatialRelations.owl#place"/>
</rdf:Description>

<rdf:Description rdf:about="http://purl.org/net/wsml/dolceSpatial#place_of">
