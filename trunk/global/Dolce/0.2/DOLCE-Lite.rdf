	<rdfs:seeAlso rdf:resource="http://www.loa-cnr.it/ontologies/DOLCE-Lite.owl#state"/>
</rdf:Description>

<rdf:Description rdf:about="http://purl.org/ifgi/dolce#temporal_quality">
	<rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
	<rdfs:label>http://purl.org/ifgi/dolce#temporal_quality</rdfs:label>
	<rdfs:subClassOf rdf:resource="http://purl.org/ifgi/dolce#quality"/>
	<dc:identifier rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Temporal Quality@en</dc:identifier>
	<dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A quality inherent in a perdurant.</dc:description>
	<rdfs:seeAlso rdf:resource="http://www.loa-cnr.it/ontologies/DOLCE-Lite.owl#temporal-quality"/>
</rdf:Description>

<rdf:Description rdf:about="http://purl.org/ifgi/dolce#temporal_location_q">
	<rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
	<rdfs:label>http://purl.org/ifgi/dolce#temporal_location_q</rdfs:label>
	<rdfs:subClassOf rdf:resource="http://purl.org/ifgi/dolce#quality"/>
	<dc:identifier rdf:datatype="http://www.w3.org/2001/XMLSchema#string">temporal location quality@en</dc:identifier>
	<dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A temporal location quality.</dc:description>
	<rdfs:seeAlso rdf:resource="http://www.loa-cnr.it/ontologies/DOLCE-Lite.owl#temporal-location_q"/>
</rdf:Description>

<rdf:Description rdf:about="http://purl.org/ifgi/dolce#temporal_region">
	<rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
	<rdfs:label>http://purl.org/ifgi/dolce#temporal_region</rdfs:label>
	<rdfs:subClassOf rdf:resource="http://purl.org/ifgi/dolce#region"/>
	<dc:identifier rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Temporal Region@en</dc:identifier>
	<dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A region at which only temporal qualities can be directly located. It assumes a metrics for time.</dc:description>
	<rdfs:seeAlso rdf:resource="http://www.loa-cnr.it/ontologies/DOLCE-Lite.owl#temporal-region"/>
</rdf:Description>

<rdf:Description rdf:about="http://purl.org/ifgi/dolce#temporal_region_prop_part">
	<rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
	<rdfs:label>http://purl.org/ifgi/dolce#part</rdfs:label>
	<rdfs:domain rdf:resource="http://purl.org/ifgi/dolce#temporal_region"/>
	<rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
	<rdfs:range rdf:resource="http://purl.org/ifgi/dolce#temporal_region"/>
</rdf:Description>

<rdf:Description rdf:about="http://purl.org/ifgi/dolce#temporal_region_prop_q_location_of">
	<rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
	<rdfs:label>http://purl.org/ifgi/dolce#q_location_of</rdfs:label>
	<rdfs:domain rdf:resource="http://purl.org/ifgi/dolce#temporal_region"/>
	<rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
	<rdfs:range rdf:resource="http://purl.org/ifgi/dolce#temporal_quality"/>
</rdf:Description>

<rdf:Description rdf:about="http://purl.org/ifgi/dolce#time_interval">
	<rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
	<rdfs:label>http://purl.org/ifgi/dolce#time_interval</rdfs:label>
	<rdfs:subClassOf rdf:resource="http://purl.org/ifgi/dolce#temporal_region"/>
	<dc:identifier rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Time Interval@en</dc:identifier>
	<dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A temporal region, measured according to a calendar.</dc:description>
	<rdfs:seeAlso rdf:resource="http://www.loa-cnr.it/ontologies/DOLCE-Lite.owl#time-interval"/>
</rdf:Description>

<rdf:Description rdf:about="http://purl.org/ifgi/dolce#immediate_relation">
	<rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
	<rdfs:domain rdf:resource="http://purl.org/ifgi/dolce#particular"/>
	<rdfs:range rdf:resource="http://purl.org/ifgi/dolce#particular"/>
	<dc:identifier rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Immediate Relation@en</dc:identifier>
	<dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relation that holds without additional mediating individuals. In logical terms, a non-composed relation.</dc:description>
	<rdfs:seeAlso rdf:resource="http://www.loa-cnr.it/ontologies/DOLCE-Lite.owl#immediate-relation"/>
</rdf:Description>

<rdf:Description rdf:about="http://purl.org/ifgi/dolce#immediate_relation_i">
	<rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
	<rdfs:domain rdf:resource="http://purl.org/ifgi/dolce#particular"/>
	<rdfs:range rdf:resource="http://purl.org/ifgi/dolce#particular"/>
	<dc:identifier rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Immediate Relation Inverse@en</dc:identifier>
	<dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relation that holds without additional mediating individuals. In logical terms, a non-composed relation.</dc:description>
	<rdfs:seeAlso rdf:resource="http://www.loa-cnr.it/ontologies/DOLCE-Lite.owl#immediate-relation-i"/>
</rdf:Description>

<rdf:Description rdf:about="http://purl.org/ifgi/dolce#mediated_relation">
	<rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
	<rdfs:domain rdf:resource="http://purl.org/ifgi/dolce#particular"/>
	<rdfs:range rdf:resource="http://purl.org/ifgi/dolce#particular"/>
	<dc:identifier rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Mediated Relation@en</dc:identifier>
	<dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relation that composes other relations. For example, a participation relation composed with a representation relation.Composed relation cannot be directly expressed in OWL-DL, then (at least some) compositions are expressed as class or restriction axioms.</dc:description>
	<rdfs:seeAlso rdf:resource="http://www.loa-cnr.it/ontologies/DOLCE-Lite.owl#mediated-relation"/>
</rdf:Description>

<rdf:Description rdf:about="http://purl.org/ifgi/dolce#mediated_relation_i">
	<rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
	<rdfs:domain rdf:resource="http://purl.org/ifgi/dolce#particular"/>
	<rdfs:range rdf:resource="http://purl.org/ifgi/dolce#particular"/>
	<dc:identifier rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Immediate Relation Inverse@en</dc:identifier>
	<dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relation that composes other relations. For example, a participation relation composed with a representation relation.Composed relation cannot be directly expressed in OWL-DL, then (at least some) compositions are expressed as class or restriction axioms.</dc:description>
	<rdfs:seeAlso rdf:resource="http://www.loa-cnr.it/ontologies/DOLCE-Lite.owl#mediated-relation-i"/>
</rdf:Description>

<rdf:Description rdf:about="http://purl.org/ifgi/dolce#generic_constituent">
	<rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
	<rdfs:domain rdf:resource="http://purl.org/ifgi/dolce#particular"/>
	<rdfs:range rdf:resource="http://purl.org/ifgi/dolce#particular"/>
	<rdfs:subPropertyOf rdf:resource="http://purl.org/ifgi/dolce#immediate_relation"/>
	<dc:identifier rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Generic Constituent@en</dc:identifier>
	<dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">'Constituent' should depend on some layering of the ontology. For example, scientific granularities or ontological 'strata' are typical layerings. A constituent is a part belonging to a lower layer. Since layering is actually a partition of the ontology, constituents are not properly classified as parts, although this kinship can be intuitive for common sense. Example of specific constant constituents are the entities constituting a setting (a situation), whilethe entities constituting a collection are examples of generic constant constituents.</dc:description>
	<rdfs:seeAlso rdf:resource="http://www.loa-cnr.it/ontologies/DOLCE-Lite.owl#generic-constituent"/>
</rdf:Description>

<rdf:Description rdf:about="http://purl.org/ifgi/dolce#generic_constituent_of">
	<rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
	<rdfs:domain rdf:resource="http://purl.org/ifgi/dolce#particular"/>
	<rdfs:range rdf:resource="http://purl.org/ifgi/dolce#particular"/>
	<rdfs:subPropertyOf rdf:resource="http://purl.org/ifgi/dolce#immediate_relation_i"/>
	<dc:identifier rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Generic Constituent Of@en</dc:identifier>
	<dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">'Constituent' should depend on some layering of the ontology. For example, scientific granularities or ontological 'strata' are typical layerings. A constituent is a part belonging to a lower layer. Since layering is actually a partition of the ontology, constituents are not properly classified as parts, although this kinship can be intuitive for common sense. Example of specific constant constituents are the entities constituting a setting (a situation), whilethe entities constituting a collection are examples of generic constant constituents.</dc:description>
	<rdfs:seeAlso rdf:resource="http://www.loa-cnr.it/ontologies/DOLCE-Lite.owl#generic-constituent-of"/>
</rdf:Description>

<rdf:Description rdf:about="http://purl.org/ifgi/dolce#generic_dependent">
	<rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
	<rdfs:domain rdf:resource="http://purl.org/ifgi/dolce#particular"/>
	<rdfs:range rdf:resource="http://purl.org/ifgi/dolce#particular"/>
	<rdfs:subPropertyOf rdf:resource="http://purl.org/ifgi/dolce#immediate_relation"/>
	<dc:identifier rdf:datatype="http://www.w3.org/2001/XMLSchema#string">generic dependent of@en</dc:identifier>
	<dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The dependence on an individual of a given type at some time. This is traditionally a relation between particulars and universals, but this one states that x generically depends on y if a z different from y, but with the same properties, can be equivalently its depend-on.This is a temporally-indexed relation (embedded in this syntax).</dc:description>
	<rdfs:seeAlso rdf:resource="http://www.loa-cnr.it/ontologies/DOLCE-Lite.owl#generic-dependent"/>
</rdf:Description>

<rdf:Description rdf:about="http://purl.org/ifgi/dolce#identitiy_c">
