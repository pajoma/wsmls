<rdf:Description rdf:about="http://purl.org/ifgi/dolce#specific_constant_constituent_of">
	<rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
	<rdfs:domain rdf:resource="http://purl.org/ifgi/dolce#particular"/>
