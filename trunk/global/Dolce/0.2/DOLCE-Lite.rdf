	<rdfs:domain rdf:resource="http://purl.org/ifgi/dolce#particular"/>
	<rdfs:range rdf:resource="http://purl.org/ifgi/dolce#particular"/>
	<rdfs:subPropertyOf rdf:resource="http://purl.org/ifgi/dolce#immediate_relation"/>
	<dc:identifier rdf:datatype="http://www.w3.org/2001/XMLSchema#string">is identitical to (Ontologically)@en</dc:identifier>
	<dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Any pair of individuals are ontologically identical if they are identical to themselves. Reflexive, symmetric, and transitive.</dc:description>
	<rdfs:seeAlso rdf:resource="http://www.loa-cnr.it/ontologies/DOLCE-Lite.owl#identitiy-c"/>
</rdf:Description>

<rdf:Description rdf:about="http://purl.org/ifgi/dolce#identitiy_n">
	<rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
	<rdfs:domain rdf:resource="http://purl.org/ifgi/dolce#particular"/>
	<rdfs:range rdf:resource="http://purl.org/ifgi/dolce#particular"/>
	<rdfs:subPropertyOf rdf:resource="http://purl.org/ifgi/dolce#immediate_relation"/>
	<dc:identifier rdf:datatype="http://www.w3.org/2001/XMLSchema#string">is identitical to (notionally)@en</dc:identifier>
	<dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Any pair of individuals are notionally identical iff they instantiate all and only the same concepts.</dc:description>
	<rdfs:seeAlso rdf:resource="http://www.loa-cnr.it/ontologies/DOLCE-Lite.owl#identitiy-n"/>
</rdf:Description>

