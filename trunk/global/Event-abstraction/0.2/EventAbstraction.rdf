<rdf:Description rdf:about="http://wsmls.googlecode.com/svn/trunk/global/Event-abstraction/0.2/EventAbstraction.rdf#isDerivedFrom">
	<rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
