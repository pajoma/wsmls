<rdf:Description rdf:about="http://purl.org/ifgi/dulSpatial#place">
	<rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
	<rdfs:label rdf:resource="http://purl.org/ifgi/dulSpatial#place"/>
	<rdfs:domain rdf:resource="http://purl.org/ifgi/dul#PhysicalObject"/>
	<rdfs:range rdf:resource="http://purl.org/ifgi/dul#PhysicalPlace"/>
	<rdfs:subPropertyOf rdf:resource="http://purl.org/ifgi/dulSpatial#approximate_location"/>
	<dc:identifier rdf:datatype="http://www.w3.org/2001/XMLSchema#string">has place@en</dc:identifier>
	<dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relation of approximate location holding between any endurant and physical endurants. It assumes a shared physical region. Sharedness can be any mereotopological association, but no analytical definition can be done in OWL due to the lack of role value maps.</dc:description>
	<rdfs:seeAlso rdf:resource="http://www.loa-cnr.it/ontologies/SpatialRelations.owl#place"/>
</rdf:Description>

<rdf:Description rdf:about="http://purl.org/ifgi/dulSpatial#placeOf">
