<rdf:Description rdf:about="http://purl.org/ifgi/dulSpatial#descriptivePlace">
	<rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
	<rdfs:label rdf:resource="http://purl.org/ifgi/dulSpatial#descriptivePlace"/>
	<rdfs:domain rdf:resource="http://purl.org/ifgi/dul#PhysicalObject"/>
	<rdfs:range rdf:resource="http://purl.org/ifgi/dul#Place"/>
	<rdfs:subPropertyOf rdf:resource="http://purl.org/ifgi/dul#hasLocation"/>
	<dc:identifier rdf:datatype="http://www.w3.org/2001/XMLSchema#string">has descriptive place@en</dc:identifier>
	<dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Approximate location of an endurant in a non-physical one. It assumes a shared region. AKA fiat-place.</dc:description>
	<rdfs:seeAlso rdf:resource="http://www.loa-cnr.it/ontologies/SpatialRelations.owl#descriptive-place"/>
</rdf:Description>

